{
    "name": "tebros/laravel-cloudflare",
    "description": "Laravel framework Service Provider to automatic loading IP ranges of Cloudflare trust proxies.",
    "keywords": ["laravel", "framework", "cloudflare", "trust", "proxy", "proxies"],
    "license": "MIT",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "≈Åukasz Adamski"
        }
    ],
    "require": {
        "php": "~7.1",
        "illuminate/support": "~5.0",
        "illuminate/console": "~5.0",
        "illuminate/http": "~5.0",
        "fideloper/proxy": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Adams\\Cloudflare\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Adams\\Cloudflare\\CloudflareServiceProvider"
            ]
        }
    }
}